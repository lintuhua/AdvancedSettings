<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    <PreferenceCategory android:title="@string/settings">
    <SwitchPreference
        android:icon="@mipmap/xui_ic_settings_control_center"
        android:persistent="false"
        android:key="double_tap"
        android:title="@string/double_tap"
        android:summary="@string/double_tap1"/>
    <SwitchPreference
        android:icon="@mipmap/xui_ic_settings_security_privacy"
        android:persistent="false"
        android:key="cover"
        android:title="@string/cover"
        android:summary="@string/cover1"/>
    <SwitchPreference
        android:icon="@mipmap/xui_ic_settings_notification_statusbar"
        android:persistent="false"
        android:key="Charging"
        android:title="@string/Charging"
        android:summary="@string/Charging1"/>
    <SwitchPreference
        android:icon="@mipmap/xui_ic_settings_wallpaper"
        android:persistent="false"
        android:key="proximity"
        android:title="@string/proximity"
        android:summary="@string/proximity1"/>
    <PreferenceScreen
        android:icon="@mipmap/xui_ic_settings_utouch"
        android:title="@string/u_touch_settings">
        <intent
            android:targetPackage="com.android.exsettings"
            android:action="android.intent.action.MAIN"
            android:targetClass="com.android.exsettings.Settings$UTounchCustomSettingsActivity" />
    </PreferenceScreen>
    <PreferenceScreen
        android:icon="@mipmap/xui_ic_settings_displaylight"
        android:title="@string/screen_color">
            <intent
                android:targetPackage="com.android.exsettings"
                android:action="android.intent.action.MAIN"
                android:targetClass="com.android.exsettings.Settings$ScreenTonePrefs" />
    </PreferenceScreen>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/about">
    <PreferenceScreen
        android:icon="@mipmap/xui_ic_settings_ota"
        android:title="@string/update">
        <intent
            android:targetPackage="com.sun.beizikeji.ota"
            android:action="android.intent.action.MAIN"
            android:targetClass="com.sun.beizikeji.ota.MainActivity">
        </intent>
    </PreferenceScreen>
    <PreferenceScreen
        android:icon="@mipmap/xui_ic_settings_about"
        android:summary="@string/author1"
        android:key="author"
        android:title="@string/author">
    </PreferenceScreen>
    <PreferenceScreen
        android:icon="@mipmap/share_alipay_all"
        android:key="pay"
        android:title="@string/pay"/>
    </PreferenceCategory>
</PreferenceScreen>